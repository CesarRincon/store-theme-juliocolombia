{
    "flex-layout.row#menu-inferior": {
        "title": "menu inferior mobile",
        "children": [
            "flex-layout.col#menu-inferior__menu",
            "flex-layout.col#menu-inferior__carrito",
            "flex-layout.col#menu-inferior__tu-cuenta",
            "flex-layout.col#menu-inferior__busqueda"
        ],
        "props": {
            "blockClass": "menu-inferior",
            "preserveLayoutOnMobile": true,
            "fullWidth": true,
            "horizontalAlign": "center"
        }
    },
    "flex-layout.col#menu-inferior__menu": {
        "title": "Contenedor menu inferior",
        "children": [
            "drawer#header__mobile--menu",
            "rich-text#inferior__menu"
        ],
        "props": {
            "blockClass": "menu-inferio_items",
            "horizontalAlign": "center"
        }
    },
    "rich-text#inferior__menu": {
        "title": "Texto menu",
        "props": {
            "text": "Menú",
            "textAlignment": "CENTER",
            "font": "t-mini",
            "blockClass": "menu-inferio_text"
        }
    },
    "flex-layout.col#menu-inferior__carrito": {
        "title": "Item carrito menu inferior",
        "children": [
            "minicart.v2#inferior__carrito",
            "rich-text#inferior__carrito"
        ],
        "props": {
            "blockClass": "menu-inferio_items",
            "horizontalAlign": "center",
            "height": 10
        }
    },
    "minicart.v2#inferior__carrito": {
        "props": {
            "MinicartIcon": "icon#home__cart-icon2",
            "itemCountMode": "total",
            "quantityDisplay": "always",
            "maxDrawerWidth": 50
        },
        "children": [
            "minicart-base-content"
        ]
    },
    "rich-text#inferior__carrito": {
        "title": "Texto carrito menu inferior",
        "props": {
            "text": "Carrito",
            "textAlignment": "CENTER",
            "font": "t-mini",
            "blockClass": "menu-inferio_text"
        }
    },
    "flex-layout.col#menu-inferior__tu-cuenta": {
        "title": "item login",
        "children": [
            "login#menu-inferior",
            "rich-text#inferior__tu-cuenta"
        ],
        "props": {
            "blockClass": "menu-inferio_items",
            "horizontalAlign": "center"
        }
    },
    "login#menu-inferior": {
        "props": {
            "hideIconLabel": true,
            "showIconProfile": true,
            "logInButtonBehavior": "link"
        }
    },
    "rich-text#inferior__tu-cuenta": {
        "title": "Texto item login ",
        "props": {
            "text": "Tú cuenta",
            "textAlignment": "CENTER",
            "font": "t-mini",
            "blockClass": [
                "menu-inferio_text",
                "tu-cuenta"
            ]
        }
    },
    "flex-layout.col#menu-inferior__busqueda": {
        "title": "item busqueda menu inferior",
        "children": [
            "drawer#search-header",
            "rich-text#inferior__busqueda"
        ],
        "props": {
            "blockClass": "menu-inferio_items",
            "horizontalAlign": "center"
        }
    },
    "rich-text#inferior__busqueda": {
        "title": "Texto item busqueda",
        "props": {
            "text": "Busqueda",
            "textAlignment": "CENTER",
            "font": "t-mini",
            "blockClass": [
                "menu-inferio_text",
                "busqueda"
            ]
        }
    }
}