{
    "drawer#header__mobile--menu": {
        "title": "drawer menu mobile",
        "children": [
            "flex-layout.row#header__drawer"
        ],
        "blocks": [
            "drawer-trigger#menu"
        ],
        "props": {
            "maxWidth": "50%",
            "blockClass": "mobile--menu"
        }
    },
    "flex-layout.row#header__drawer": {
        "children": [
            "mega-menu#mobile"
        ]
    },
    
    "drawer-trigger#menu": {
        "title": "icono del drawer menu",
        "children": [
            "image#trigger__icon-menu"
        ]
    },
    "image#trigger__icon-menu": {
        "props": {
            "src": "assets/img/menu-icon.ico",
            "blockClass": "trigger__icon-menu"
        }
    },

    "mega-menu#mobile": {
        "props": {
            "title": "MENÃš",
            "defaultDepartamentActive": "..."
        }
    }
